;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MineMark"
config/tags=PackedStringArray("20_games")
run/main_scene="res://menus/menu.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://assets/splash.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

Console="*res://addons/godot-console/console/src/Console.tscn"
Print="*res://addons/sdg-print/Print.tscn"
Globals="*res://globals.tscn"
WorldGen="*res://chunking/world_gen.tscn"
FpsCounter="*res://addons/fps_counter/FpsCounter.tscn"

[debug]

file_logging/max_log_files=500

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/canvas_textures/default_texture_filter=1

[editor_plugins]

enabled=PackedStringArray("res://addons/fps_counter/plugin.cfg", "res://addons/godot-console/console/plugin.cfg", "res://addons/sdg-print/plugin.cfg", "res://addons/thegates_export/plugin.cfg")

